{
  "name": "findmykorki",
  "version": "1.0.0",
  "main": "app/index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "test:ci": "jest --ci --runInBand",
    "format": "prettier --write app/",
    "prepare": "husky"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|@react-native-async-storage/async-storage)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@gorhom/bottom-sheet": "^5.0.6",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@shopify/flash-list": "^1.7.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/react": "^18.2.6",
    "date-fns": "^4.1.0",
    "axios": "^1.9.0",
    "dayjs": "^1.11.13",
    "expo": "~52.0.37",
    "expo-linking": "~7.0.5",
    "expo-secure-store": "~14.0.1",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "^0.27.3",
    "expo-web-browser": "~14.0.2",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.4",
    "nativewind": "^4.1.23",
    "react": "^18.3.1",
    "react-i18next": "^15.4.1",
    "react-native": "^0.76.8",
    "react-native-axios": "^0.17.1",
    "react-native-paper": "^5.14.1",
    "react-native-svg": "^15.11.2",
    "react-native-vector-icons": "^10.2.0",
    "tailwindcss": "^3.4.17",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "expo-image-picker": "~16.0.6",
    "react-native-calendar-picker": "^8.0.5",
    "react-native-reanimated": "3.16.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "latest",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "babel-preset-expo": "^12.0.10",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
